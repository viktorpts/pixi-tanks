{
    "env": {
        "browser": true,
        "node": true,
        "mocha": true,
        "es2021": true
    },
    "globals": {
        "expect": true
    },
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": "error",
        "curly": "error",
        "quotes": [
            "warn",
            "single"
        ],
        "no-undef": "error"
    }
}